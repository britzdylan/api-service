    "Api Service",
    "typescript"
  ],
  "author": "Dylan Britz",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "commitizen": "^4.3.1",
    "conventional-changelog-cli": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "gh-release": "^7.0.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  }
